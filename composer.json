{
    "require": {
        "twig/twig": "^3.0",
        "monolog/monolog": "^2.0",
        "symfony/runtime": "^6.0",
        "symfony/orm-pack": "^2.3",
        "symfony/twig-bundle": "*",
        "symfony/finder": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.50"
    }
}
